<head>
	<title></title>
	<script type="text/javascript">
	

	function valuesGreater(arr){
		var values = 0;
		for(var i = 0; i < arr.length; i++){
			if(i>arr[1]){
				console.log(i);
				values = values + 1;
			}
			
		}
		return(values);
	}
	valuesGreater([1,3,5,7,9,13]);

	</script>
	}
	}
	
	
